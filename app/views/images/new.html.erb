<h1>Images#new</h1>
<p>Find me in app/views/images/new.html.erb</p>


<%= form_for [@user, @image], :html => {:multipart => true} do |f| %>
  
  <p>
    <%= f.file_field :file %>
  </p>
  <p>
    <%= f.label :remote_image_url, "or image URL" %><br />
    <%= f.text_field :remote_image_url %>
  </p>
  <p><%= f.submit %></p>
<% end %>